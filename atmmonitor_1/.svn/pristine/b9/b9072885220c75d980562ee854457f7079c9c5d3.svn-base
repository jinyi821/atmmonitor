package com.common.core.component.rule.operator;



public class AndOperator extends Operator {

	public AndOperator()
	{
		super("and",2);
	}
	
	public double evaluate(final double leftOperand, final double rightOperand)
	{
		if (leftOperand == 1 && rightOperand == 1) {
			return 1;
		}
		return 0;
	}	
}
