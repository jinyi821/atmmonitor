@CHARSET "UTF-8";

.contentTab{
	margin-bottom: 15px;
	height: 38px;
	line-height: 38px;
	border: 1px solid #eeeeee;
	background: #f8f8f8;
}

.spbtn{
	border-right: 1px solid #eeeeee;
	border-left: 1px solid #eeeeee;
	margin-left: -4px;
}

