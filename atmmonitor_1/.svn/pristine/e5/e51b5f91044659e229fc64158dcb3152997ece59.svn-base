public class CombineQuery {
	
	String vendorAndType;
	String region;
	
	
	public String getVendorAndType() {
		return vendorAndType;
	}
	public void setVendorAndType(String vendorAndType) {
		this.vendorAndType = vendorAndType;
	}
	public String getRegion() {
		return region;
	}
	public void setRegion(String region) {
		this.region = region;
	}
	@Override
	public String toString() {
		return "CombineQuery [vendorAndType=" + vendorAndType + ", region=" + region + "]";
	}
	
	
}
