<!-- 采集历史明细（包括当天） -->
<div role="content">

    <!-- widget edit box -->
    <div class="jarviswidget-editbox">
        <!-- This area used as dropdown edit box -->
         
    </div>
    <!-- end widget edit box -->
     
    <!-- widget content -->
    <div class="widget-body">
        <table id="displayTable" class="table table-bordered table-striped " style="clear: both">
            <tbody>
            <tr>
                <td >ID</td>
                <td ><label class=" control-label" id="fileId"></label></td>
            </tr>
            <tr>
                <td>IP地址</td>
                <td><label class="control-label" id="ftpIp"></label></td>
            </tr>
            <tr>
                <td>Probe名称</td>
                <td><label class="control-label" id="probeName"></label></td>
            </tr>
            <tr>
                <td>数据类型</td>
                <td><label class="control-label" id="dataTypeName"></label></td>
            </tr>
            <tr>
                <td>全路径文件
                </td>
                <td><label class="control-label" id="pathAndName1"></label></td>
            </tr>

            <tr>
                <td>文件更改后的名称</td>
                <td><label class="control-label" id="renameFileName"></label></td>
            </tr>
            <tr>
                <td>生成时间</td>
                <td><label class="control-label" id="fileInsertTime"></label></td>
            </tr>
            <tr>
                <td>状态</td>
                <td><label class="control-label" id="status"></label></td>
            </tr>
            <tr>
                <td>文件大小(B)</td>
                <td><label class="control-label" id="fileSize"></label></td>
            </tr>
            <tr>
                <td>FTP文件路径</td>
                <td><label class="control-label" id="ftpPathAndName"></label></td>
            </tr>
            <tr>
                <td>下载时长(ms)</td>
                <td><label class="control-label" id="downloadDuration"></label></td>
            </tr>
            <tr>
                <td>上传时长(ms)</td>
                <td><label class="control-label" id="toHdfsDuration"></label></td>
            </tr>
            <tr>
                <td>文件存储HDFS路径</td>
                <td><label class="control-label" id="hdfsPath"></label></td>
            </tr>
            </tbody>
        </table>

    </div>
    <!-- end widget content -->

</div>