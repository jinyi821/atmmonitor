        <input type="text"  class="form-control input-sm duiqi" id="dataTypePathReg" placeholder="输入正则表达式">
    </div>
</div>
<div class="form-group">
    <label for="isFtpDelete" class="col-xs-4 control-label">是否删除ftp文件：</label>
    <div class="col-xs-8 ">
        <div class="form-contro input-sm duiqi">
            <label class="radio-inline">
                <input type="radio" name="isFtpDelete" id="optionsRadios1" value="1" checked> 是
            </label>
            <label class="radio-inline">
                <input type="radio" name="isFtpDelete" id="optionsRadios2"  value="0"> 否
