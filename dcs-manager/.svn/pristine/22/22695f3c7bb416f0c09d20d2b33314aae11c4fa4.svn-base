<div role="content">

    <!-- widget edit box -->
    <div class="jarviswidget-editbox">
        <!-- This area used as dropdown edit box -->
         
    </div>
    <!-- end widget edit box -->

    <!-- widget content -->
    <div class="widget-body">

        
        <table id="displayTable" class="table table-bordered table-striped" style="clear: both">
            <tbody>
            <tr>
                <td >ID</td>
                <td ><label class="control-label" id="fileId"></label></td>
            </tr>
            <tr>
                <td>FTP名称</td>
                <td><label class="control-label" id="ftpName"></label></td>
            </tr>
            <tr>
